<form action="{{ path('fos_user_profile_edit') }}" {{ form_enctype(form) }} method="POST"
      class="fos_user_profile_edit">

    {{ form_start(form) }}

    {{ form_errors(form) }}

    {{ form_row(form.subscription) }}
    {{ form_row(form.username) }}
    {{ form_row(form.businessName) }}
    {{ form_row(form.email) }}
    {{ form_row(form.title) }}
    {{ form_row(form.firstName) }}
    {{ form_row(form.lastName) }}
    <div class="hidden">
        {{ form_row(form.country) }}
    </div>
    {{ form_row(form.timezone) }}

    {{ form_row(form.current_password) }}

    {{ form_end(form) }}
</form>
