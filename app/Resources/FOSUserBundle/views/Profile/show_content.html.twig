<div class="fos_user_user_show">

    {% embed "buttons.html.twig" %}
        {% block buttons %}
            <a href="{{ path('fos_user_profile_edit') }}" class="btn btn-primary">{{ 'profile.edit.label'|trans({}, 'FOSUserBundle') }}</a>
        {% endblock %}
    {% endembed  %}

    <div>{{ 'myhs.subscription.label'|trans }}: {{ user.subscription }}</div>
    <div>{{ 'profile.show.username'|trans({}, 'FOSUserBundle') }}: {{ user.username }}</div>
    <div>{{ 'profile.show.email'|trans({}, 'FOSUserBundle') }}: {{ user.email }}</div>
    <div>{{ 'myhs.title.label'|trans }}: {{ user.title }}</div>
    <div>{{ 'myhs.user.first_name'|trans }}: {{ user.firstName }}</div>
    <div>{{ 'myhs.user.last_name'|trans }}: {{ user.lastName }}</div>
    <div>{{ 'myhs.country.label'|trans }}: {{ user.country }}</div>
    <div>{{ 'myhs.timezone.label'|trans }}: {{ user.timezone }}</div>
    <div>{{ 'profile.show.api_key'|trans({}, 'FOSUserBundle') }}: {{ user.apiKey }}</div>

</div>
