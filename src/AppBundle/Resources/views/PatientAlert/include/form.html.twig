<form name="{{ form.vars.name }}" method="post" class="app-patient-alert-form"
      action="{{ entity.id ? path('patient_alert_update',{'id':app_hash(entity)}) : path('patient_alert_create',{'id': app_hash(entity.patient)}) }}" {{ form_enctype(form) }} >
    {# form_start(form) #}

    {{ form_row(form.text) }}

    <div class="app-buttons-placeholder">
        {% include "@App/PatientAlert/include/buttons.html.twig" %}
    </div>

    {{ form_widget(form._token) }}
    {{ form_end(form, {'render_rest': false}) }}
