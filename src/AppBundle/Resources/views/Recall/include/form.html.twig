<form name="{{ form.vars.name }}" method="post" class="app-patient-recall-form"
      action="{{ entity.id ? path('recall_update',{'id':app_hash(entity)}) : path('patient_recall_create',{'patient': app_hash(entity.patient)}) }}" {{ form_enctype(form) }} >
    {# form_start(form) #}

    {{ form_row(form.recallType) }}
    {{ form_row(form.date) }}
    {{ form_row(form.recallFor) }}
    {{ form_row(form.text) }}

    <div class="app-buttons-placeholder">
        {% include "@App/Recall/include/buttons.html.twig" %}
    </div>

    {{ form_widget(form._token) }}
    {{ form_end(form, {'render_rest': false}) }}
