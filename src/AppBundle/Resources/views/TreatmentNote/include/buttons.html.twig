{% set cancelPath = path('treatment_note_index', {'id':app_hash(entity.patient)}) %}

<button type="submit"
        class="btn btn-labeled btn-success app-tn-save-final"><span
            class="btn-label"><i
                class="glyphicon glyphicon-ok"></i></span>{{ 'app.treatment_note.save_final'|trans }}</button>

<button type="submit"
        class="btn btn-labeled btn-default app-tn-save-draft"><span
            class="btn-label"><i
                class="glyphicon glyphicon-ok"></i></span>{{ 'app.treatment_note.save_draft'|trans }}</button>

<a href="{{ cancelPath }}" class="btn btn-labeled btn-danger"><span
            class="btn-label"><i
                class="glyphicon glyphicon-remove"></i></span>{{ 'app.action.cancel'|trans }}</a>

