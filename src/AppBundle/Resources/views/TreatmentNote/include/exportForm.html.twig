<form name="{{ form.vars.name }}" method="post" class="app-modal-treatment-note-export-form"
      action="" {{ form_enctype(form) }} >
    {# form_start(form) #}

    <div id="range-selector">
        {{ form_row(form.range) }}
    </div>

    <div class="row no-gutter ranges display-none">
        <div class="col-md-4">
            {{ form_row(form.dateStart) }}
        </div>
        <div class="col-md-4">
            {{ form_row(form.dateEnd) }}
        </div>
        <div class="col-md-4">
            <a href="#" class="btn btn-default btn-block apply-range">{{ 'app.action.apply'|trans }}</a>
        </div>
    </div>

    <div class="notes">
        <table class="table">
            <tbody>

            </tbody>
        </table>
    </div>

    {{ form_widget(form._token) }}
    {{ form_end(form, {'render_rest': false}) }}
