{% extends 'inner.html.twig' %}

{% block header %}
    {{ 'myhs.treatment_note_template.plural_label'|trans }}
{% endblock %}

{% block body %}

    {% embed "buttons.html.twig" %}
        {% block buttons %}
            <a href="{{ path('treatment_note_template_create') }}" class="btn btn-primary">{{ 'myhs.treatment_note_template.new'|trans }}</a>
        {% endblock %}
    {% endembed  %}

    <table class="table table-hover table-bordered">
        <thead>
        <tr>
            <th>Name</th>
            <th>Actions</th>
        </tr>
        </thead>
        <tbody>
        {% for entity in treatmentNoteTemplates %}
            <tr>
                <td><a href="{{ path('treatment_note_template_view', { 'id': entity.id }) }}">{{ entity }}</a></td>
                <td>
                    <a href="{{ path('treatment_note_template_update', { 'id': entity.id }) }}">Edit</a> /
                    <a href="{{ path('treatment_note_template_delete', { 'id': entity.id }) }}">Delete</a>
                    </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

{% endblock %}
