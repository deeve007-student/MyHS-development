{% extends 'inner.html.twig' %}

{% block header %}
    {{ 'app.treatment_note_template.plural_label'|trans }}
{% endblock %}

{% block body %}

    {% embed "buttons.html.twig" %}
        {% block buttons %}
            <a href="{{ path('treatment_note_template_create') }}" class="btn btn-primary">{{ 'app.treatment_note_template.new'|trans }}</a>
        {% endblock %}
    {% endembed  %}

    <div class="navigation">
        {{ knp_pagination_render(treatmentNoteTemplates) }}
    </div>

    <table class="table table-hover table-bordered">
        <thead>
        <tr>
            <th>{{ 'app.treatment_note_template.name'|trans }}</th>
        </tr>
        </thead>
        <tbody>
        {% for entity in treatmentNoteTemplates %}
            <tr>
                <td><a href="{{ path('treatment_note_template_view', { 'id': entity.id }) }}">{{ entity }}</a></td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

{% endblock %}
