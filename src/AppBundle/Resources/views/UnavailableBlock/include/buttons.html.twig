{% if not entity.id %}
    <button type="button" class="btn btn-default app-appointment-create">{{ 'app.appointment.new'|trans }}</button>
{% endif %}

<button type="submit" onclick="$('form[name={{ form.vars.name }}]').trigger('submit'); return false;"
        class="btn btn-labeled btn-success"><span
            class="btn-label"><i
                class="glyphicon glyphicon-ok"></i></span>{{ 'app.action.save'|trans }}</button>

<button type="button" class="btn btn-labeled btn-danger" data-dismiss="modal"><span
            class="btn-label" data-dismiss="modal"><i
                class="glyphicon glyphicon-remove"></i></span>{{ 'app.action.cancel'|trans }}</button>
