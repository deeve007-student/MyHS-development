{% extends '@Report/layout.html.twig' %}

{% block body %}
    <div class="row report-grid">

        {% for route,name in routes %}
            <div class="col-sm-4 col-lg-3">
                <div class="report-grid-item">
                    <a href="{{ path(route) }}" class="grid-item-content">
                        {{ name|trans }}
                    </a>
                </div>
            </div>
        {% endfor %}

    </div>
{% endblock %}
